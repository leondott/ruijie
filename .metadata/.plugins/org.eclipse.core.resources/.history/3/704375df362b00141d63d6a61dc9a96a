package com.ruijie.rush.model;

public class Category {

	private String author = "";
	private float price = 0;
	private int pages = 0;
	private String name = "";
	
	public Category(String author, String name, float price, int pages) {
		this.author = author;
		this.name = name;
		this.price = price;
		this.pages =pages;
	}
	
	public String getAuthor() {
		return author;
	}
	
	public void setAuthor(String author) {
		this.author = author;
	}
	
	public float getPrice() {
		return price;
	}
	
	public void setPrice(float price) {
		this.price = price;
	}
	
	public int getPages() {
		return pages;
	}
	
	public void setPages(int pages) {
		this.pages = pages;
	}
	
	public String getName() {
		return name;
	}
	
	public void setName(String name) {
		this.name = name;
	}
	
}
